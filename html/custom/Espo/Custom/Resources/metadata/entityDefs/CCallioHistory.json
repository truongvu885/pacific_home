{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "fromNumber": {
            "type": "varchar",
            "maxLength": 12,
            "options": [],
            "isCustom": true
        },
        "toNumber": {
            "type": "varchar",
            "maxLength": 12,
            "options": [],
            "isCustom": true
        },
        "startTime": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "endTime": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "duration": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "billDuration": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "hangupCause": {
            "type": "enum",
            "options": [
                "USER_BUSY",
                "ORIGINATOR_CANCEL",
                "NORMAL_TEMPORARY_FAILURE",
                "NORMAL_CLEARING"
            ],
            "style": {
                "USER_BUSY": "danger",
                "ORIGINATOR_CANCEL": "danger",
                "NORMAL_TEMPORARY_FAILURE": "danger",
                "NORMAL_CLEARING": "success"
            },
            "default": "USER_BUSY",
            "maxLength": 100,
            "isCustom": true
        },
        "transcripts": {
            "type": "array",
            "storeArrayValues": true,
            "default": [],
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}