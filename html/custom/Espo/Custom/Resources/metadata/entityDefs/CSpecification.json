        },
        "houseType": {
            "type": "varchar",
            "maxLength": 50,
            "options": [],
            "isCustom": true
        },
        "landArea": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "constructionArea": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "size": {
            "type": "varchar",
            "maxLength": 50,
            "options": [],
            "isCustom": true
        },
        "direction": {
            "type": "varchar",
            "maxLength": 50,
            "options": [],
            "isCustom": true
        },
        "floors": {
            "type": "int",
            "isCustom": true
        },
        "floor1": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "floor2": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "floor3": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "floor4": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "floor5": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "floor6": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "projectCode": {
            "type": "varchar",
            "maxLength": 20,
            "options": [],
            "isCustom": true
        },
        "street": {
            "type": "varchar",
            "maxLength": 20,
            "options": [],
            "isCustom": true
        },
        "streetNumber": {
            "type": "varchar",
            "maxLength": 20,
            "options": [],
            "isCustom": true
        },
        "houseTemplate": {
            "type": "varchar",
            "maxLength": 50,
            "options": [],
            "isCustom": true
        },
        "usableArea": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "fullArea": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "streetEven": {
            "type": "int",
            "max": 1,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}